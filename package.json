{
  "name": "unplugin-inline-enum",
  "type": "module",
  "version": "0.6.4",
  "packageManager": "pnpm@10.29.2",
  "description": "Inline enum values to optimize bundle size.",
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/sxzz",
  "homepage": "https://github.com/unplugin/unplugin-inline-enum#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-inline-enum.git"
  },
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-inline-enum/issues"
  },
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./api": "./dist/api.mjs",
    "./esbuild": "./dist/esbuild.mjs",
    "./rolldown": "./dist/rolldown.mjs",
    "./rollup": "./dist/rollup.mjs",
    "./vite": "./dist/vite.mjs",
    "./webpack": "./dist/webpack.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "ast-kit": "^2.2.0",
    "magic-string": "^0.30.21",
    "picomatch": "^4.0.3",
    "tinyglobby": "^0.2.15",
    "unplugin": "^2.3.11",
    "unplugin-replace": "^0.6.3"
  },
  "devDependencies": {
    "@babel/types": "^7.29.0",
    "@sxzz/eslint-config": "^7.7.0",
    "@sxzz/prettier-config": "^2.3.1",
    "@sxzz/test-utils": "^0.5.15",
    "@types/node": "^25.2.2",
    "@types/picomatch": "^4.0.2",
    "@typescript/native-preview": "7.0.0-dev.20260209.1",
    "bumpp": "^10.4.1",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "rollup": "^4.57.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "unplugin-oxc": "^0.5.7",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "webpack": "^5.105.0"
  },
  "prettier": "@sxzz/prettier-config"
}
